{
  "name": "claude-code-dev-stack",
  "version": "3.7.5",
  "description": "AI-powered development environment with 37 specialized agents, intelligent hooks, and unified tooling",
  "main": "index.js",
  "type": "module",
  "bin": {
    "ccds-setup": "./bin/ccds-setup.cjs",
    "ccds-detect": "./bin/detect-platform.js",
    "ccds-install-hooks": "./bin/install-hooks.js",
    "ccds-start": "./bin/ccds-start.js",
    "ccds-stop": "./bin/ccds-stop.js",
    "ccds-tunnel-start": "./bin/ccds-tunnel-start.js",
    "ccds-tunnel-stop": "./bin/ccds-tunnel-stop.js",
    "ccds-tunnel-status": "./bin/ccds-tunnel-status.js",
    "ccds-tunnel-restart": "./bin/ccds-tunnel-restart.js",
    "ccds-tunnel-config": "./bin/ccds-tunnel-config.js",
    "ccds-agents": "./bin/ccds-agents.js",
    "ccds-hooks": "./bin/ccds-hooks.js",
    "ccds-router": "./bin/ccds-router.js",
    "ccds-integrate": "./bin/ccds-integrate.js",
    "ccds-test": "./bin/ccds-test.js"
  },
  "workspaces": [
    "src/apps/*",
    "src/core/*",
    "scripts"
  ],
  "scripts": {
    "setup": "node bin/ccds-setup.cjs",
    "setup:all": "node scripts/install/complete-install.js",
    "setup:hooks": "node bin/install-hooks.js",
    "setup:fix": "node bin/detect-platform.js --fix",
    "test:vm": "bash scripts/test-vm.sh",
    "start": "npm run start:services",
    "start:services": "node scripts/services/start-all-services.js",
    "start:web": "cd src/apps/web && npm run dev",
    "start:backend": "cd src/apps/backend && npm start",
    "start:terminal": "node scripts/services/start-terminal-server.js",
    "start:tunnel": "node bin/claude-code-tunnel-start.js",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit": "cd tests && npm run unit",
    "test:integration": "cd tests && npm run integration",
    "clean": "node scripts/cleanup/master-cleanup.js",
    "validate": "node scripts/validation/final-validation.js",
    "build": "cd src/apps/web && npm run build",
    "dev": "cd src/apps/web && npm run dev",
    "dev:full": "concurrently \"npm run start:terminal\" \"npm run dev\"",
    "tunnel:start": "node bin/claude-code-tunnel-start.js",
    "tunnel:stop": "node bin/claude-code-tunnel-stop.js",
    "tunnel:status": "node bin/claude-code-tunnel-status.js",
    "tunnel:restart": "node bin/claude-code-tunnel-restart.js",
    "tunnel:config": "node bin/claude-code-tunnel-config.js"
  },
  "keywords": [
    "ai",
    "development",
    "agents",
    "automation",
    "claude",
    "hooks",
    "orchestration",
    "devops",
    "backend",
    "frontend",
    "fullstack",
    "productivity",
    "workflow",
    "tooling",
    "enterprise"
  ],
  "author": "Claude Code Team <support@claude-code.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KrypticGadget/Claude_Code_Dev_Stack.git"
  },
  "homepage": "https://github.com/KrypticGadget/Claude_Code_Dev_Stack",
  "bugs": {
    "url": "https://github.com/KrypticGadget/Claude_Code_Dev_Stack/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "yaml": "^2.3.4",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "chokidar": "^3.5.3",
    "node-cron": "^3.0.3",
    "shelljs": "^0.8.5",
    "semver": "^7.5.4",
    "validate-npm-package-name": "^5.0.0",
    "ws": "^8.14.2",
    "concurrently": "^8.2.2",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "clipboardy": "^3.0.0"
  },
  "optionalDependencies": {
    "node-pty": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "peerDependenciesMeta": {
    "python3": {
      "optional": true
    },
    "git": {
      "optional": true
    }
  },
  "files": [
    "index.js",
    "bin/",
    "scripts/",
    "lib/",
    "core/agents/",
    "core/hooks/",
    "core/audio/",
    "ui/react-pwa/",
    "templates/",
    "config/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "claude-code": {
    "version": "3.0.0",
    "agents": 28,
    "hooks": 37,
    "audioFiles": 90,
    "features": [
      "agent-orchestration",
      "smart-routing",
      "audio-feedback",
      "unified-pwa",
      "performance-monitoring",
      "security-scanning",
      "auto-documentation",
      "quality-gates"
    ]
  }
}